{
  "name": "storybook",
  "version": "1.0.0",
  "description": "front end application for story teller server side",
  "main": "index.js",
  "scripts": {
    "start": "node src/index.js",
    "prod": "env-cmd -f ./config/prod.env node src/index.js",
    "dev": "env-cmd -f ./config/dev.env nodemon src/index.js",
    "test": "env-cmd -f ./config/test.env jest --detectOpenHandles --runInBand"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.1.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "got": "^12.1.0",
    "hbs": "^4.0.1",
    "jsonwebtoken": "8.5.1",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "validator": "^13.0.0"
  },
  "devDependencies": {
    "env-cmd": "^10.1.0",
    "nodemon": "^2.0.3",
    "jest": "^26.0.1",
    "supertest": "^4.0.2"
  },
